                        </div>
                        <nav>
                          <div class="nav nav-tabs diet-tabs" id="nav-tab" role="tablist">
                            <% Exercise.exercise_types.keys.each_with_index do |type, idx| %>
                                <button class="nav-link <%= 'active' if idx == 0 %>" id="nav-home-tab" data-bs-toggle="tab" data-bs-target="#nav-home" type="button" role="tab" aria-controls="nav-home" aria-selected="true"><%= type.humanize %></button>
                            <% end %>
                          </div>
                        </nav>
                        <div class="tab-content diet-content" id="nav-tabContent">
                          <div class="tab-pane fade show active" id="nav-home" role="tabpanel" aria-labelledby="nav-home-tab" tabindex="0">
                            <div class="card-body loadmore-content dz-scroll height750"  id="DietMenusContent">

                              <% @exercises.each do |ex| %>
                                <div class="media border-bottom mb-3 pb-3 d-lg-flex d-block menu-list">
                                  <%= link_to exercise_thumb(ex, width: 150, height: 120, class_name: 'rounded me-3 mb-md-0 mb-3'), ex %>

                                  <div class="food-default-flex col-lg-8 ps-0">
                                    <h6 class="fs-16 font-w600"><a href="ecom-product-detail.html" class="text-black"><%= link_to ex.name, ex %></a></h6>
                                    <p class="fs-14"><%= link_to ex.description, ex %></p>
                                    <div class="d-flex flex-wrap align-items-center">
                                      <div class="d-flex mb-sm-2 mb-3 pe-3 me-auto align-items-center">
                                        <span class="fs-14 text-black font-w500"><%= ex.movement_pattern.humanize %> â€¢ <%= ex.exercise_type.humanize %></span>
                                      </div>
                                      <ul class="d-flex flex-wrap mb-sm-0 mb-2">
                                        <li class="mb-2 me-4 text-nowrap"><i class="las la-dumbbell scale5 me-2"></i>
                                          <span class="fs-14 text-black text-nowrap font-w500"><%= ex.equipment.map(&:humanize).join(', ') %></span>
                                        </li>
                                        <li class="mb-2 me-4 text-nowrap"><i class="las la-male scale5 me-2"></i><span class="text-nowrap fs-14 text-black font-w500"> <%= ex.primary_muscles.map(&:humanize).join(', ') %></span></li>
                                     </ul>
                                      <% badge_class, badge_label = case ex.technical_complexity
                                                                    when "high" then ["badge-danger", "High"]
                                                                    when "medium" then ["badge-warning", "Medium"]
                                                                    else ["badge-success", "Low"]
                                                                    end %>
                                      <span class="badge <%= badge_class %> light mb-1 mb-sm-0"><%= badge_label %>-Complexity</span>


                                    </div>
                                  </div>
                                  <a href="javascript:void(0);" data-bs-toggle="modal" data-bs-target="#aAddDietMenus" class="btn btn-primary light btn-md ms-auto"><i class="fa fa-plus scale5 me-3"></i> To Workout</a>
                                </div>
                              <% end %>

                            </div>
                            <div class="card-footer style-1 text-center border-0 pt-0 pb-4">
                              <a class="text-primary dz-load-more fa fa-chevron-down" id="DietMenus" href="javascript:void(0);" rel="ajax/food-menu-list.html">
                              </a>
                            </div>
                          </div>

